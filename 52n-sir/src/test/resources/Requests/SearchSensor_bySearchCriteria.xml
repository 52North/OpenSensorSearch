<?xml version="1.0" encoding="UTF-8"?>
<sir:SearchSensorRequest service="SIR"
	sir:version="0.3.2" xmlns:sir="http://52north.org/sir/0.3.2" xmlns:ows="http://www.opengis.net/ows/1.1"
	xmlns:swe="http://www.opengis.net/swe/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://52north.org/sir/0.3.2 http://52north.org/schema/sir/0.3.2/sirAll.xsd">
	<sir:SearchCriteria>
		<sir:ServiceCriteria>
			<sir:ServiceURL>http://v-swe.uni-muenster.de:8080/WeatherSOS/sos</sir:ServiceURL>
			<sir:ServiceType>SOS</sir:ServiceType>
		</sir:ServiceCriteria>
		<sir:Phenomenon>
			<sir:PhenomenonName>urn:x-ogc:def:property:OGC::RelativeHumidity</sir:PhenomenonName>
		</sir:Phenomenon>
		<sir:BoundingBox crs="urn:ogc:def:crs:EPSG:6.14:4326"
			dimensions="2">
			<ows:LowerCorner>-180.0 -90.0</ows:LowerCorner>
			<ows:UpperCorner>180.0 90.0</ows:UpperCorner>
		</sir:BoundingBox>
	</sir:SearchCriteria>
</sir:SearchSensorRequest>
