<rim:RegistryObjectList xmlns:rim="urn:oasis:names:tc:ebxml-regrep:xsd:rim:3.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:oasis:names:tc:ebxml-regrep:xsd:rim:3.0 http://docs.oasis-open.org/regrep/v3.0/schema/rim.xsd">

	<!--
		See
		http://code.google.com/p/buddata-ebxml-registry/issues/detail?id=58
		for a discussion of how to add slots to buddata.
	-->
	<rim:ExtrinsicObject id="urn:sir:id:SlotDefinition"
		objectType="urn:kzen:ObjectType:SlotDescription">

		<!--
			See OGC document 09-163, section "Slots" for details on the slots.
		-->
		<rim:Slot name="urn:ogc:def:slot:OGC-CSW-ebRIM-Sensor::ShortName"
			slotType="string">
			<rim:ValueList>
				<rim:Value />
			</rim:ValueList>
		</rim:Slot>
		<rim:Slot name="urn:ogc:def:slot:OGC-CSW-ebRIM-Sensor::Keywords"
			slotType="string">
			<rim:ValueList>
				<rim:Value />
			</rim:ValueList>
		</rim:Slot>
		<rim:Slot
			name="urn:ogc:def:slot:OGC-CSW-ebRIM-Sensor::ServiceSpecificSensorID"
			slotType="string">
			<rim:ValueList>
				<rim:Value />
			</rim:ValueList>
		</rim:Slot>
		<rim:Slot name="urn:ogc:def:slot:OGC-CSW-ebRIM-Sensor::Inputs"
			slotType="string">
			<rim:ValueList>
				<rim:Value />
			</rim:ValueList>
		</rim:Slot>
		<rim:Slot name="urn:ogc:def:slot:OGC-CSW-ebRIM-Sensor::Outputs"
			slotType="string">
			<rim:ValueList>
				<rim:Value />
			</rim:ValueList>
		</rim:Slot>

		<rim:Slot name="urn:ogc:def:slot:OGC-CSW-ebRIM-Sensor::BoundedBy"
			slotType="geometry">
			<rim:ValueList>
				<rim:Value />
			</rim:ValueList>
		</rim:Slot>
		<rim:Slot name="urn:ogc:def:slot:OGC-CSW-ebRIM-Sensor::Location"
			slotType="geometry">
			<rim:ValueList>
				<rim:Value />
			</rim:ValueList>
		</rim:Slot>
		<rim:Slot name="urn:ogc:def:slot:OGC-CSW-ebRIM-Sensor::ValidTimeBegin"
			slotType="datetime">
			<rim:ValueList>
				<rim:Value />
			</rim:ValueList>
		</rim:Slot>
		<rim:Slot name="urn:ogc:def:slot:OGC-CSW-ebRIM-Sensor::ValidTimeEnd"
			slotType="datetime">
			<rim:ValueList>
				<rim:Value />
			</rim:ValueList>
		</rim:Slot>

	</rim:ExtrinsicObject>
</rim:RegistryObjectList>
